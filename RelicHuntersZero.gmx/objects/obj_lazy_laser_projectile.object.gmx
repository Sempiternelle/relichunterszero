            <string>///Main Shit
owner = noone;
damage = 0;
precisionDamage = 0;
range = 0;
decay = 0;
type = 0;
push_power = 0;
faction = f_player;
distance_travelled = 0;

allowPrecision = true;
piercing = 0;

bounce = false;
bounceMax = 1;
bounceCurrent = 0;

